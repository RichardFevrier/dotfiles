{
    "colorscheme": "bogster",
    "diffgutter": true,
    "ft:markdown": {
        "rmtrailingws": false
    },
    "indentchar": "|",
    "lsp.autocompleteDetails": false,
    "lsp.ignoreMessages": "LS message1 to ignore|LS message 2 to ignore|...",
    "lsp.ignoreTriggerCharacters": "completion,signature",
    "lsp.server": "python=pylsp",
    "lsp.tabcompletion": true,
    "mkparents": true,
    "quoter.enable": true,
    "rmtrailingws": true,
    "statusformatr": "$(percentage)%, $(bind:ToggleKeyMenu): bindings, $(bind:ToggleHelp): help",
    "tabmovement": true,
    "tabstospaces": true
}
